PROCEDURE InsertionSort(arr)  ()
VAR
    
BEGIN
   WHILE (j>=0 and arr[j]> key) DO
 arr [j+1] = arr[j]
 j= j-1
END_WHILE
    arr [j+1] = key
END
ALGORITHM tri simple
VAR
    
    arr : ARRAY_OF INTEGER[];
    INTEGER: key, i , j ;
BEGIN
 InsertionSort(arr)
 n:= length (arr)
FOR i FROM 1 TO n  DO
    
    key:= arr[i];
    j:=i-1;

END_FOR

END

